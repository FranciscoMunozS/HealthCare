<div id="healths">
  <h1 class="title">Programas de salud</h1>
  <hr>
  <% if user_signed_in? %>
    <%= link_to 'Agregar nuevo programa', new_health_path, class: 'btn btn-success' %>
    <hr>
  <% end %>
    <% @healths.each do |health| %>
      <div class="health">
        <h2 class="title"><%= link_to health.title, health %></h2>
      </div>
    <% end %>
</div>

<p id="notice"><%= notice %></p>

<h1>Programas de salud</h1>

<hr>

<div class="row">
  <% @healths.each do |health| %>
    <div class="col-md-4">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <%= health.title %>
        </div>
        <div class="panel-body">
          <p><%= health.title %></p>
        </div>
        <div class="panel-footer text-center">
          <%= link_to 'Ver plan de salud', health, class: 'btn btn-default'  %>
        </div>
      </div>
    </div>
  <% end %>
</div>
